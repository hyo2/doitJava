package innerclass;

class OutClass{
	
	private int num = 10;
	private static int sNum = 20;
	private InClass inClass;
	
	public OutClass() {
		inClass = new InClass();
	}
	
	// 인스턴스 inner 클래스: 멤버변수와 동일한 위치에 선언되어 있음
	class InClass{
		
	}
}

